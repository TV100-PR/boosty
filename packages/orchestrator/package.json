{
  "name": "@boosty/mcp-orchestrator",
  "version": "1.0.0",
  "description": "Volume orchestration and bot coordination for DeFi MCP servers",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@boosty/mcp-shared": "workspace:*",
    "@boosty/mcp-solana-core": "workspace:*",
    "bullmq": "^5.13.0",
    "ioredis": "^5.4.1",
    "node-cron": "^3.0.3",
    "prom-client": "^15.1.3",
    "uuid": "^10.0.0",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^10.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "@boosty/mcp-trading-engine": "workspace:*",
    "@boosty/mcp-wallet-manager": "workspace:*"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "defi",
    "mcp",
    "orchestrator",
    "trading",
    "bots",
    "solana"
  ],
  "license": "MIT"
}
